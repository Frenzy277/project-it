<section class="wrapper">
  <%= render 'shared/content_title', title: "All Projects" %>


<ul>
  <% @projects.each do |project| %>
    <li><%= project.project_name %> [<%= link_to "Like", like_project_path(project), method: 'post' %>]
      <br>(<%= link_to "show project", project_path(project) %>)
      <br>(<%= link_to "management", management_project_path(project) %>)

    </li>
  <% end %>
</ul>

</section>